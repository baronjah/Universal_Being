	mat.emission_enabled = true
	mat.emission = color
	mat.emission_energy = 1.0
	body.material_override = mat
	node.add_child(body)
	
	# INTERACTIVE LABEL
	var label = Label3D.new()
	label.text = name
	label.billboard = BaseMaterial3D.BILLBOARD_ENABLED
