func _ready():
	print('üß™ TESTING FALLBACK SYSTEM')
	
	# Wait for auto-registration to complete
	await get_tree().process_frame
	
	# Create player instance with fallback
	var player = UniversalFallbackSystem.create_fallback_instance('BasePlayer')
	
	if player:
		print('‚úÖ Player created with fallback system')
		
		# Test method calls
		if player.has_method('move_player'):
			player.move_player(1.0)
		
		if player.has_method('attack'):
			player.attack()
		
		if player.has_method('fly'):
			player.fly()
		
		if player.has_method('cast_spell'):
			player.cast_spell('Lightning')
		
		if player.has_method('get_health'):
			print('Health: ' + str(player.get_health()))
		
		if player.has_method('get_mana'):
			print('Mana: ' + str(player.get_mana()))
	else:
		print('‚ùå Failed to create player instance')
