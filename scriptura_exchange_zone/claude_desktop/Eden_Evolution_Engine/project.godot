; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the properties are organized in sections here.

[application]

config/name="Eden Evolution Engine - Magnum Opus"
config/description="Multi-dimensional evolution game with Akashic Records, JSH Console, and 12-turn system"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"
config/version="4.0-SONNET-OPUS"

[autoload]

AkashicCore="*res://akashic_records/akashic_core.gd"
JSHCore="*res://jsh_console/jsh_core.gd"
EvolutionCore="*res://evolution_core/evolution_engine.gd"
UniverseGenerator="*res://universe_generator/universe_factory.gd"
TwelveTurns="*res://twelve_turns_system/turns_manager.gd"
Notepad3D="*res://notepad3d_system/notepad_3d.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=true
window/dpi/allow_hidpi=true
window/vsync/use_vsync=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/eden_tools/" )

[global]

version="4.0"
multicore=true
api_connections=[ "claude-opus-4", "gpt-4", "local" ]

[gui]

theme/use_hidpi=true
theme/custom="res://themes/eden_theme.tres"

