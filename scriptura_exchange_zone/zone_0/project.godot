; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="12 Turns System"
config/description="A turn-based cosmic creation system exploring multi-dimensional evolution through 12 phases of reality. Features multi-threaded word processing, dimensional visualization, and sacred geometry integration."
config/tags=PackedStringArray("cosmic", "creation", "multi-dimensional", "sacred", "word-processing")
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.5")
config/icon="res://icon.svg"

[autoload]

ThreadManager="*res://core/thread_manager.gd"
WordProcessorTasks="*res://core/word_processor_tasks.gd"
TurnSystem="*res://turn_system.gd"
DimensionalColorSystem="*res://dimensional_color_system.gd"
UniversalConnector="*res://universal_connector.gd"
WordManifestationSystem="*res://word_manifestation_system.gd"
MemoryInvestmentSystem="*res://memory_investment_system.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="12 Turns System"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[filesystem]

import/blender/enabled=false

